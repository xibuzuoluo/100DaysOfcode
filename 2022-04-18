/***************************************
Find special natural numbers.
****************************************/
#include <iostream>
using namespace std;

int main()
{
	int i;
	for (i = 65; i <= 342; i++)
	{
		if((i%9==i/49) && (i/9%9 == i/7%7) && (i/81 == i%7))
		{
			cout << i << endl;
			cout << i/49 << i/7%7 << i%7 << endl;
			cout << 8/81 << 8/9%9 << 8%9 << endl;
			break;
		}
	}
	return 0;
} 
