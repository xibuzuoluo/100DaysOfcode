/*********************************
Simple calculator
**********************************/

#include<iostream>
using namespace std;

int main(){
	double a=0, b=0, res=0;
	char c;
	cin >> a >> b;
	cin >> c;

	if(c == '+'){
		res = a+b;
		cout << res;
	}
	else if(c == '-'){
		res = a-b; 
		cout << res;
	} 
	else if(c == '*'){
		res = a*b;
		cout << res;
	}
	else if(c == '/'){
		if(b==0){
			cout << "The denominator cannot be 0";
		}
		else{
		res = a/b;
		cout << res;
		}
	}
	else
		{
		cout << "Wrong";
	}
}
