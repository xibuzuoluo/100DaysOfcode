/************************************
Palindrome Judgment.
*************************************/
#include <bits/stdc++.h>
using namespace std;
int main()
{
	string s;
	cin >> s;
	int num = 0;
	for(int i=0; i<s.size();i++)
		num += s[i] - '0';
	if(num%3==0) cout << "Good" << endl;
	else cout << "Bad" << endl;
	return 0; 
}
