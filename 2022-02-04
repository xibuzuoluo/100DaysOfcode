/**************************************************************************
Tomorrow never knowsï¼Ÿ
Read a date in the format YYYY-MM-DD (that is, year-month-day) 
and print the date of the day following that date.
***************************************************************************/
#include<iostream>
using namespace std;

int main(){
	int y=0, m=0, d=0;
	char a,b;
	cin >> y >> a >> m >> b >> d;
	if(m==12 && d==31){
		y=y+1;
		m=1;
		d=1;
	} 
	else if(y % 4 == 0 && y % 100 != 0 || y % 400 == 0 && m==2 && d==29){
		m=m+1;
		d=1;
	}
	else if(y%4!=0 && m==2 && d==28){
		m=m+1;
		d=1;
	} 
	else if(m<=7 && m%2!=0 && d ==30){
		m=m+1;
		d=1;
	}
	else if(m<=7 && m%2==0 && d==31){
		m=m+1;
		d=1;
	}
	else if(m>=7 && m%2!=0 && d==31){
		m=m+1;
		d=1;
	}
	else if(m>=7 && m%2==0 && d==30){
		m=m+1;
		d=1;
	}
	else if(y<=0 || m<=0 || d<=0){
		cout << "Wrong input!";	
		return 0;	
	}
	else if(m>12){
		cout << "Wrong input!";	
		return 0;
	}
	else if(y%4==0 && m==2 && d>29){
		cout << "Wrong input!";
		return 0;
	}
	else if(y%4!=0 && m==2 && d>28){
		cout << "Wrong input!";
		return 0;
	} 
	else if(m<=7 && m%2!=0 && d>30){
		cout << "Wrong input!";
		return 0;
	}
	else if(m<=7 && m%2==0 && d>31){
		cout << "Wrong input!";
		return 0;
	}
	else if(m>=7 && m%2!=0 && d>31){
		cout << "Wrong input!";
		return 0;
	}
	else if(m>=7 && m%2==0 && d>30){
		cout << "Wrong input!";
		return 0;
	}
	else{
		d=d+1;
	}	
	cout << y << "-" << m << "-" << d << endl;
	return 0;
}
