/***********************************
Address of one-dimensional array
************************************/
#include<iostream>
using namespace std;

int main(){
	int a[5] = {1,2,3,4,5};
	cout << a << endl;
	cout << a+1 << endl;
	cout << &a << endl;
	cout << &a+1 << endl;
	cout << *(&a)+1 << endl;
	return 0;
}
