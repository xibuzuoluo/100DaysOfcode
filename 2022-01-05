/* output the array in reverse order */
#include<iostream>
using namespace std;
int a[100];

int main(){
	int n;
	
	cin >> n;
	for(int i=0; i < n; i++)
		cin >> a[i];
	while (n--){ // Common reverse counting loop, equivalent to while(n-->0)
		cout << a[n];
		if (n>0) cout << " "; //If it's not the last number, separate it with a space
		
	}	
	return 0;
	
	
}
