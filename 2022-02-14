/************************************
Generate Fibonacci Sequence.
*************************************/
#include<iostream>
using namespace std;

int f(int n){

	if(n == 1)
		return 1;
	if(n == 2)
		return 1;
	else 
		return(f(n-1) + f(n-2));
	return(n);
}

int main(){
	cout << "Input a number: ";
	int m;
	cin >> m;
	f(m);
	cout << f(m);
}
