/*****************************************
Count the number of vowels in the input text.
******************************************/

#include<iostream>
#include<string.h>
using namespace std;

int main(){
//	int m;
//	cin >> m;
//	char s[m];
	char s [100];
	int n=0,a_n=0,e_n=0,i_n=0,o_n=0,u_n=0;
	gets(s);
	n = strlen(s);
	for(int i=0; i<n; i++){
		if(s[i] == 'a')
		a_n++;
		if(s[i] == 'e')
		e_n++;
		if(s[i] == 'i')
		i_n++;
		if(s[i] == 'o')
		o_n++;
		if(s[i] == 'u')
		u_n++; 		
	}
	cout << "a_n" << ' ' << a_n << endl;
	cout << "e_n" << ' ' << e_n << endl;
	cout << "i_n" << ' ' << i_n << endl;
	cout << "o_n" << ' ' << o_n << endl;
	cout << "u_n" << ' ' << u_n << endl;
}
